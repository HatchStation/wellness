<div class="box we-diet span4" ontablet="span6" ondesktop="span4">
  <div class="we-title">
    diet
  </div>
  <div class="we-content">
    <div class="we-row">
      <span class="f-title">
        vegetables
      </span>
      <span class="pull-right fd-qua">
        4
        <span> servings </span>
      <i class="fa fa-chevron-right"></i>
      </span>
      <div class="progress">
        <div class="progress-bar well-bg-yellow" role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100" style="width: 30%"></div>
      </div>
    </div>


    <div class="we-row">
      <span class="f-title">
        fruit
      </span>
      <span class="pull-right  fd-qua">
        <%= risks['FruitRisk'] %>
        <span>pieces</span>

      <i class="fa fa-chevron-right"></i>
      </span>

      <div class="progress">
        <div class="progress-bar well-bg-yellow" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%"></div>
      </div>
    </div>

    <div class="we-row">
      <span class="f-title">
        dairy
      </span>
      <span class="pull-right fd-qua">
        <%= risks['DairyRisk'] %>
        <span>cups</span>

      <i class="fa fa-chevron-right"></i>
      </span>

      <div class="progress">
        <div class="progress-bar well-bg-blue" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 45%"></div>
      </div>
    </div>

    <div class="we-row">
      <span class="f-title">
        red meat
      </span>
      <span class="pull-right  fd-qua">
        <%= risks['meatrisk'] %>
        <span>pieces</span>

      <i class="fa fa-chevron-right"></i>
      </span>

      <div class="progress">
        <div class="progress-bar well-bg-red" role="progressbar" aria-valuenow="98" aria-valuemin="0" aria-valuemax="100" style="width: 98%"></div>
      </div>
    </div>
    <div style="text-align: center">
      <span class="pull-left">
        <i class="fa fa-chevron-left"></i>
        yesterday
      </span>

      today

      <span class="pull-right">
        right
      <i class="fa fa-chevron-right"></i>
      </span>

    </div>
  </div>
</div>
